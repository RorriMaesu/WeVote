{
  "compilerOptions": {
  "module": "commonjs",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "target": "ES2020",
    "outDir": "lib",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../",
  "moduleResolution": "node",
    "paths": {}
  },
  "include": ["src"],
  "exclude": ["node_modules", "lib"]
}
