# Functions environment variables (copy to .env and fill in values)
# Gemini API Key (required for draft generation)
GEMINI_API_KEY=replace_me
# Optional: Skip LLM calls (use stub JSON) for local testing
SKIP_LLM=0
# Receipt HMAC secret (fallback when no KMS signing)
RECEIPTS_SECRET=replace_me_secret
# KMS signing key path (optional) e.g. projects/PROJECT/locations/global/keyRings/wevote-core/cryptoKeys/wevote-signing/cryptoKeyVersions/1
KMS_KEY_PATH=
